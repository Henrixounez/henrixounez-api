{
  "name": "henrixounez-api",
  "version": "1.0.0",
  "description": "Henrixounez Website API",
  "scripts": {
    "start": "node build/index.js",
    "start-ts": "ts-node src/index.ts",
    "postinstall": "tsc --outDir build",
    "watch-node": "nodemon build/index.js",
    "watch-ts": "tsc -w",
    "watch": "nodemon -e ts,tsx,js,jsx,graphql --watch src/ --exec npm run start-ts"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "pg": "^8.5.1",
    "querystring": "^0.2.0",
    "typeorm": "^0.2.31",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.9",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/express-ws": "^3.0.0",
    "@types/node": "^12.19.16",
    "@types/uuid": "^8.3.1",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.7"
  }
}
